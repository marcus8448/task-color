{
  "manifest_version": 3,
  "name": "Task Colour",
  "author": "Marcus Low",
  "description": "Allows for recolouring of tasks in Google Calendar\u2122",
  "version": "0.1.0",
  "content_scripts": [
    {
      "js": [ "main.js" ],
      "matches": [ "https://calendar.google.com/*" ]
    }
  ],
  "icons": {
    "16": "icon/16.png",
    "48": "icon/48.png",
    "128": "icon/128.png"
  },
  "permissions": [ "storage" ]
}
